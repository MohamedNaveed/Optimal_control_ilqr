% mpc using ilqr

clear;clc;

model = model_register('1dcos');
model.name
fprintf('initial state = %d \n', model.X0);
fprintf('final state = %d \n', model.Xg);

fprintf('Horizon = %d \n', model.horizon);
fprintf('dt = %d \n', model.dt);


epsi_range = 0:0.2:2;

n_mc_runs = 100;

cost_epsi = zeros(length(epsi_range),1)

for epsilon = epsi_range
    
    cost_vec = zeros(n_mc_runs,1);
    
    for n_mc = 1:n_mc_runs
       
        cost_vec(n_mc) = mpc_ilqr(model, epsilon);
    end
    
end





